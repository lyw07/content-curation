    "clean-webpack-plugin": "^0.1.19",
    "commander": "^2.11.0",
    "css-loader": "^0.28.11",
    "cypress": "^3.0.1",
    "eslint": "^4.1.1",
    "eslint-plugin-require-path-exists": "^1.1.7",
    "eslint-plugin-vue": "^3.5.0",
    "esprima": "^4.0.0",
    "glob": "^7.1.2",
    "handlebars-template-loader": "^1.0.0",
    "imports-loader": "^0.8.0",
