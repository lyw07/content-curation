{
  "name": "studio",
  "version": "0.0.1",
  "private": true,
  "description": "Frontend build tools and development utilities for Kolibri Studio",
  "scripts": {
    "lint:py": "flake8 --exit-zero",
    "makemessages": "node ./extract_frontend_messages.js ./contentcuration/contentcuration/static/js/edit_channel/**{/*,}.{js,vue}",
    "build": "webpack --config webpack.config.js",
    "cypress:run": "wait-on -t 20000 tcp:8081 && cypress run --spec cypress/integration/login.js",
    "cypress:test": "yarn run build && npm-run-all --parallel --race testserver cypress:run",
    "pytest": "cd contentcuration; pytest",
    "pytest:reusedb": "cd contentcuration; pytest --reuse-db",
    "postgres": "service postgresql start || pg_ctl -D /usr/local/var/postgresql@9.6 start || true",
    "redis": "service redis-server start || redis-server /usr/local/etc/redis.conf || true",
    "services": "npm-run-all -c --parallel --silent minio redis postgres",
    "unittests": "yarn run build && yarn run pytest",
    "unittests:reusedb": "npm-run-all --parallel --race services pytest:reusedb",
    "apptests": "npm-run-all --parallel --race services cypress:test",
    "test": "npm-run-all unittests apptests",
    "build:dev": "webpack --config webpack.config.js --watch --progress",
    "minio:test": "rm -rf ~/.minio_test && minio server ~/.minio_test/ || true",
    "minio": "minio server ~/.minio_data/ || true",
    "runserver": "wait-on -t 5000 tcp:9000 && (cd contentcuration && python manage.py runserver --settings=contentcuration.dev_settings 0.0.0.0:8080)",
    "testserver": "wait-on -t 5000 tcp:9000 && (cd contentcuration && python manage.py runserver --settings=contentcuration.dev_settings 0.0.0.0:8081) || true",
    "devserver": "npm-run-all --parallel services build:dev runserver"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/learningequality/studio.git"
  },
  "keywords": [
    "education",
    "content",
    "learning",
    "edtech"
  ],
  "author": "Learning Equality",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/learningequality/studio/issues"
  },
  "homepage": "https://github.com/learningequality/studio#readme",
  "dependencies": {
    "backbone": "^1.2.1",
    "backbone-undo": "^0.2.5",
    "bootstrap": "^3.3.5",
    "browser-filesaver": "^1.1.0",
    "croppie": "^2.4.1",
    "d3": "^5.4.0",
    "d3-cloud": "^1.2.5",
    "dropzone": "^4.0.1",
    "factor-bundle": "^2.4.1",
    "fastclick": "^1.0.6",
    "get-installed-path": "^2.0.3",
    "handlebars": "^4.0.11",
    "handlebars-intl": "^1.1.2",
    "jqcloud2": "^2.0.3",
    "jquery": "^2.2.4",
    "jquery-file-download": "^1.4.6",
    "jquery-ui": "^1.12.0",
    "jszip": "^2.5.0",
    "jszip-utils": "0.0.2",
    "katex": "^0.7.0",
    "marked": "^0.3.5",
    "material-design-icons": "^3.0.1",
    "mathjax": "^2.7.0",
    "mathquill": "^0.10.1-a",
    "node-vibrant": "^3.0.0",
    "offline-js": "^0.7.19",
